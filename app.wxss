/**app.wxss**/
page{
  width: 100%;
  height: 100%;
}
.container {
  min-height: 100%;
  box-sizing:border-box;
}
/* 头部样式 */
.pageHead{
  position: fixed;
  width: 100%;
  left: 0;
  top: 0;
  height: 116rpx;
  background: #fff;
  z-index:9999;
}
.pageHead .goBackImg{
  position: absolute;
  left: 20rpx;
  top: 56rpx;
  width: 34rpx;
  height: 34rpx;
  z-index:2;
}
.pageHead .headerTitle{
  position: absolute;
  width: 100%;
  text-align: center;
  font-size: 30rpx;
  color: #5a5a5a;
  top:56rpx;
  line-height: 48rpx;
}